{
  "name": "element-lowcode-v1",
  "description": "一个element-ui 表单和表格的低代码整合组件项目",
  "version": "1.0.2",
  "private": false,
  "license": "MIT",
  "main": "lib/element-lowcode-v1.common.js",
  "files": [
    "dist",
    "node_modules",
    "public",
    "src",
    "packages"
  ],
  "scripts": {
    "serve": "vue-cli-service serve src/main.js",
    "build": "vue-cli-service build --dest dist",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name element-lowcode-v1 --dest lib packages/index.js"
  },
  "repository": {
    "type": "git",
    "directory": "https://github.com/Amos-F/element-lowcode-v1"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "element-ui": "^2.15.10",
    "moment": "^2.29.4",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.19",
    "@vue/cli-plugin-eslint": "~4.5.19",
    "@vue/cli-service": "~4.5.19",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
